{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "anhskohbo/no-captcha": "^3.2",
        "arcanedev/log-viewer": "^7.0",
        "beyondcode/laravel-self-diagnosis": "^1.4",
        "fideloper/proxy": "4.4.0",
        "fruitcake/laravel-cors": "2.0.1",
        "geoip2/geoip2": "^2.10",
        "guzzlehttp/guzzle": "6.5.5",
        "hisorange/browser-detect": "4.2.2",
        "jenssegers/mongodb": "4.0.0-alpha.1",
        "laravel/framework": "7.28.0",
        "laravel/helpers": "1.2.0",
        "laravel/horizon": "4.3.3",
        "laravel/socialite": "^4.4",
        "laravel/tinker": "2.4.1",
        "laravel/ui": "^2.1",
        "league/flysystem-sftp": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mad-web/laravel-initializer": "^3.1",
        "pavel-mironchik/laravel-backup-panel": "^1.5",
        "pragmarx/version": "^1.2",
        "predis/predis": "1.1.1",
        "propaganistas/laravel-phone": "4.2.4",
        "rap2hpoutre/fast-excel": "^2.0",
        "sentry/sentry-laravel": "1.8.0",
        "spatie/laravel-backup": "^6.11",
        "spatie/laravel-permission": "3.13.0",
        "spatie/laravel-responsecache": "^6.6",
        "spatie/laravel-schedule-monitor": "^1.0",
        "spatie/laravel-translatable": "^4.4",
        "spatie/opening-hours": "^2.8",
        "spatie/regex": "^1.4",
        "spatie/ssl-certificate": "^1.19",
        "the-tinderbox/clickhouse-builder": "^3.0",
        "torann/geoip": "1.2.1",
        "tymon/jwt-auth": "^1.0",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^10.2",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.3.3",
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/laravel-dump-server": "^1.4",
        "facade/ignition": "^2.3.4",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "app-install": [
            "@composer install",
            "@composer run-script post-root-package-install",
            "@php artisan app:install -v"
        ],
        "app-update": [
            "@php artisan app:update -v"
        ],
        "build": [
            "chown -R www-data:www-data *",
            "chmod 777 -R storage/",
            "chmod 777 -R bootstrap/cache/",
            "php artisan config:clear"
        ],
        "run-dev": [
            "service cron stop",
            "php artisan horizon:pause"
        ],
        "clear": [
            "@php artisan config:clear -n --ansi",
            "@php artisan cache:clear -n --ansi",
            "@php artisan route:clear -n --ansi",
            "@php artisan view:clear -n --ansi",
            "@php artisan clear-compiled -n --ansi",
            "@php artisan optimize:clear -n --ansi",
            "@composer dump-autoload --optimize -n --ansi",
            "@composer run-script post-autoload-dump --ansi"
        ]
    }
}
